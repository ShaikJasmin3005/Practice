class Main 
{ 
  public static void main(String[] args) 
  {
    int length = 20;
    int breadth = 2;
    int side = 4;
    int radius = 6 ;
    double pi = 3.14;
    System.out.println("Perimeter of rectangle is "+(2* (length+ breadth) ));
    System.out.println("Perimeter of square is "+ (4* side));
    System.out.println("Perimeter of circle is "+(2*pi* radius));
}
}
