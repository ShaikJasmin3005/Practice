class Main
{
  public static void main(String[] args) 
  {
     int length = 20;
     int breadth = 2;
     int side = 4;
     int radius = 6;
     double pi = 3.14;
     System.out.println("Area of rectangle is " + (length * breadth));
     System.out.println("Area of square is " + (side * side));
     System.out.println("Area of circle is " + (pi * radius * radius));
    }
}
